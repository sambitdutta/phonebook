<% present contact do |contact_presenter| %>
  <div class="col-lg-12 col-md-12 col-xs-12 contact-panel">
      <div class="panel panel-green">
          <div class="panel-heading">
              <div class="row">
                  <div class="col-xs-7">
                      <h4><%= contact_presenter.full_name %></h4>
                  </div>
                  <div class="col-xs-5 text-right mt-12">
                      
                    <%= link_to "<i class='glyphicon glyphicon-pencil white'></i>".html_safe, edit_contact_path(contact), class: "ml-10 ", title: "Edit"%>
                    <%= link_to "<i class='glyphicon glyphicon-trash white'></i>".html_safe, "javascript:void(0);", data: {url: contact_path(contact)}, class: "ml-10 delete-contact", title: "Delete" %>
                      
                  </div>
              </div>
          </div>
          <%= link_to(contact_path(contact)) do %>
            <div class="panel-footer">
                <p class="pull-left">Phone: <span class=""><%= contact_presenter.primary_phone %></span></p>
                <span class="pull-right" title="View Details"><i class="fa fa-arrow-circle-right"></i></span>
                <div class="clearfix"></div>
            </div>
          <% end #link_to%>
      </div>
  </div>
<%end%>
